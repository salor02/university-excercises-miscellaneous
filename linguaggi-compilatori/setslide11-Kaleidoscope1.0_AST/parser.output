    7    | . exp
    8 definition: . "def" proto exp
    9 external: . "extern" proto
   13 exp: . exp "+" exp
   14    | . exp "-" exp
   15    | . exp "*" exp
   16    | . exp "/" exp
   17    | . idexp
   18    | . "(" exp ")"
   19    | . "number"
   20 idexp: . "id"
   21      | . "id" "(" optexp ")"

